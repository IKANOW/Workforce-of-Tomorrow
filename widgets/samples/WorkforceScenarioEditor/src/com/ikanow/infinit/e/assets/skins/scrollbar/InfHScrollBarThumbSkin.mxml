<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	minWidth="15"
	height="16"
	alpha.disabled="0.5">

	<fx:Metadata>
		<![CDATA[ 
        [HostComponent("spark.components.Button")]
        ]]>
	</fx:Metadata>

	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>

	<s:Rect 
		bottom="1"
		left="1"
		right="1"
		top="1"
		verticalCenter="0"
		radiusX="5"
		radiusY="5">
		<s:fill>
			<s:SolidColor 
				color="0xA4A7AC"
				color.over="0xB9BDC2" />
		</s:fill>
	</s:Rect>

</s:SparkButtonSkin>
