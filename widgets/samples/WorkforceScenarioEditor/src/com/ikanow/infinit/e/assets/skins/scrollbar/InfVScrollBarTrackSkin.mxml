<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	width="16"
	minHeight="35"
	alpha.disabled="0.5">

	<fx:Metadata>
		<![CDATA[ 
        [HostComponent("spark.components.Button")]
        ]]>
	</fx:Metadata>

	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>

	<s:Rect 
		width="100%"
		height="100%"
		radiusX="6"
		radiusY="6">
		<s:fill>
			<s:SolidColor color="0x353C42" />
		</s:fill>
	</s:Rect>

</s:SparkButtonSkin>
